<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".PropertyValuesHolderTest.KeyframeTestActivity">
    <Button
        android:id="@+id/keychange"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="KEYFRAME"
        android:textSize="18sp"/>
    <Button
        android:id="@+id/OfObjectButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="18sp"
        android:text="Object"/>
    <com.example.adminstator.myviewdesign.PropertyValuesHolderTest.MyTextView
        android:id="@+id/mytextVIew"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <ImageView
        android:id="@+id/phone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/niuniu"/>

</LinearLayout>